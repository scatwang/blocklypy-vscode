{
  "name": "blocklypy",
  "displayName": "BlocklyPy Commander for LEGO Robots",
  "description": "Interact with LEGOÂ® Hubs running Pybricks firmware: compile and upload Python scripts, convert LEGO robotics files.",
  "version": "0.6.4",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "pybricks",
    "blocklypy",
    "lego",
    "ev3",
    "spike",
    "spike prime",
    "mindstorms",
    "robotics",
    "micropython",
    "python",
    "visual programming",
    "first lego league",
    "fll",
    "education",
    "world robot olympiad",
    "wro"
  ],
  "publisher": "afarago",
  "sponsor": {
    "url": "https://www.github.com/sponsors/afarago",
    "label": "Sponsor"
  },
  "homepage": "https://attilafarago.hu",
  "repository": {
    "type": "git",
    "url": "https://github.com/afarago/blocklypy-vscode.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afarago/blocklypy-vscode/issues"
  },
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:python",
    "onLanguage:lego",
    "onWebviewPanel:blocklypy-blocklypyViewer",
    "onWebviewPanel:blocklypy-pythonPreview",
    "onWebviewPanel:blocklypy-datalogview"
  ],
  "main": "./dist/extension.js",
  "packageManager": "yarn@1.22.19",
  "files": [
    "dist",
    "asset/",
    "screenshot/"
  ],
  "contributes": {
    "commands": [
      {
        "command": "blocklypy.connectDevice",
        "title": "BlocklyPy Commander: Connect Device",
        "icon": {
          "light": "asset/icons/hub-small-light.svg",
          "dark": "asset/icons/hub-small-dark.svg"
        }
      },
      {
        "command": "blocklypy.disconnectDevice",
        "title": "BlocklyPy Commander: Disconnect Device",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "blocklypy.compile",
        "title": "BlocklyPy Commander: Compile",
        "icon": "$(compile)"
      },
      {
        "command": "blocklypy.compileAndRun",
        "title": "BlocklyPy Commander: Compile and Run",
        "icon": "$(debug-start)"
      },
      {
        "command": "blocklypy.startUserProgram",
        "title": "BlocklyPy Commander: Start User Program",
        "icon": "$(run)"
      },
      {
        "command": "blocklypy.stopUserProgram",
        "title": "BlocklyPy Commander: Stop User Program",
        "icon": "$(debug-stop)"
      },
      {
        "command": "blocklypy.toggleAutoConnect",
        "title": "BlocklyPy Commander: Toggle Auto-Connect",
        "icon": "$(plug)",
        "category": "blocklypy.Settings"
      },
      {
        "command": "blocklypy.blocklypyViewer.displayNextView",
        "title": "BlocklyPy Commander: Next View",
        "icon": "$(preview)"
      },
      {
        "command": "blocklypy.blocklypyViewer.displayPreviousView",
        "title": "BlocklyPy Commander: Previous View",
        "icon": "$(preview)"
      },
      {
        "command": "blocklypy.blocklypyViewer.displayPycode",
        "title": "BlocklyPy Commander: Show Python Code",
        "icon": {
          "light": "asset/icons/display-py-light.svg",
          "dark": "asset/icons/display-py-dark.svg"
        }
      },
      {
        "command": "blocklypy.blocklypyViewer.displayPseudo",
        "title": "BlocklyPy Commander: Show Pseudo Code",
        "icon": {
          "light": "asset/icons/display-pseudo-light.svg",
          "dark": "asset/icons/display-pseudo-dark.svg"
        }
      },
      {
        "command": "blocklypy.blocklypyViewer.displayPreview",
        "title": "BlocklyPy Commander: Show Preview",
        "icon": {
          "light": "asset/icons/display-preview-light.svg",
          "dark": "asset/icons/display-preview-dark.svg"
        }
      },
      {
        "command": "blocklypy.blocklypyViewer.displayGraph",
        "title": "BlocklyPy Commander: Show Dependency Graph",
        "icon": {
          "light": "asset/icons/display-graph-light.svg",
          "dark": "asset/icons/display-graph-dark.svg"
        }
      },
      {
        "command": "blocklypy.showPythonPreview",
        "title": "BlocklyPy Commander: Show Python Dependency Preview",
        "icon": {
          "light": "asset/icons/display-graph-light.svg",
          "dark": "asset/icons/display-graph-dark.svg"
        }
      },
      {
        "command": "blocklypy.pythonPreview.showSource",
        "title": "BlocklyPy Commander: Show Python Source Code",
        "icon": "$(go-to-file)"
      },
      {
        "command": "blocklypy.clearAllSlots",
        "title": "BlocklyPy Commander: Clear All Slots in HubOS",
        "icon": "$(trash)"
      },
      {
        "command": "blocklypy.startScanning",
        "title": "BlocklyPy Commander: Start Scanning",
        "icon": "$(eye)"
      },
      {
        "command": "blocklypy.stopScanning",
        "title": "BlocklyPy Commander: Stop Scanning",
        "icon": "$(eye-closed)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "blocklypy-activitybar",
          "title": "BlocklyPy Commander",
          "icon": "asset/icons/icon.svg"
        }
      ],
      "panel": [
        {
          "id": "blocklypy-datalogview-panel",
          "title": "Datalog",
          "icon": "$(graph-line)"
        }
      ]
    },
    "views": {
      "blocklypy-datalogview-panel": [
        {
          "id": "blocklypy-datalogview",
          "name": "Chart",
          "icon": "$(graph-line)",
          "type": "webview",
          "when": "blocklypy.isPlotDataAvailable"
        }
      ],
      "blocklypy-activitybar": [
        {
          "id": "blocklypy-commands",
          "name": "Commands",
          "icon": "asset/icons/icon.svg",
          "initialSize": 20
        },
        {
          "id": "blocklypy-devices",
          "name": "Devices",
          "icon": "asset/icons/icon.svg",
          "initialSize": 20
        },
        {
          "id": "blocklypy-settings",
          "name": "Settings",
          "icon": "asset/icons/icon.svg",
          "initialSize": 10
        }
      ]
    },
    "keybindings": [
      {
        "command": "blocklypy.compileAndRun",
        "key": "f5",
        "when": "(editorLangId == python || activeCustomEditorId == blocklypy-blocklypyViewer) && blocklypy.isConnected"
      },
      {
        "command": "blocklypy.compile",
        "key": "ctrl+f5",
        "when": "(editorLangId == python || activeCustomEditorId == blocklypy-blocklypyViewer)"
      },
      {
        "command": "blocklypy.stopUserProgram",
        "key": "f6",
        "when": "blocklypy.isConnected && blocklypy.isRunning"
      },
      {
        "command": "blocklypy.stopUserProgram",
        "key": "shift+f5",
        "when": "blocklypy.isConnected && blocklypy.isRunning"
      },
      {
        "command": "blocklypy.blocklypyViewer.displayNextView",
        "key": "f7",
        "when": "activeCustomEditorId == blocklypy-blocklypyViewer"
      },
      {
        "command": "blocklypy.blocklypyViewer.displayPreviousView",
        "key": "shift+f7",
        "when": "activeCustomEditorId == blocklypy-blocklypyViewer"
      },
      {
        "command": "blocklypy.showPythonPreview",
        "key": "f7",
        "when": "editorLangId == python"
      },
      {
        "command": "blocklypy.pythonPreview.showSource",
        "key": "f7",
        "when": "activeCustomEditorId == blocklypy-pythonPreview"
      }
    ],
    "configuration": {
      "title": "BlocklyPy",
      "properties": {
        "blocklypy.lastconnected-device": {
          "type": "string",
          "default": "",
          "description": "The name of the last connected device."
        },
        "blocklypy.autoconnect-device": {
          "type": "boolean",
          "default": false,
          "description": "Automatically connect to the last connected device when opening a Python file."
        },
        "blocklypy.autostart-program": {
          "type": "boolean",
          "default": false,
          "description": "Automatically start the user program after connecting to a device."
        },
        "blocklypy.autoclear-terminal": {
          "type": "boolean",
          "default": false,
          "description": "Automatically clear the terminal before compiling and running a program."
        },
        "blocklypy.autosave-plot": {
          "type": "boolean",
          "default": false,
          "description": "Automatically save plot data to a CSV file when receiving #plot commands."
        },
        "blocklypy.connection-timeout": {
          "type": "number",
          "default": 15000,
          "minimum": 5000,
          "description": "The timeout in milliseconds for connecting to a device."
        },
        "blocklypy.device-visibility-timeout": {
          "type": "number",
          "default": 10000,
          "minimum": 1000,
          "description": "The time in milliseconds after which a device is considered not visible if not seen during scanning."
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "blocklypy.startScanning",
          "group": "navigation",
          "when": "view == blocklypy-devices && !blocklypy.isScanning"
        },
        {
          "command": "blocklypy.stopScanning",
          "group": "navigation",
          "when": "view == blocklypy-devices && blocklypy.isScanning"
        }
      ],
      "editor/title/run": [
        {
          "command": "blocklypy.compileAndRun",
          "group": "navigation",
          "when": "(editorLangId == python || activeCustomEditorId == blocklypy-blocklypyViewer) && !blocklypy.isRunning && blocklypy.isConnected"
        },
        {
          "command": "blocklypy.stopUserProgram",
          "group": "navigation",
          "when": "(editorLangId == python || activeCustomEditorId == blocklypy-blocklypyViewer) && blocklypy.isRunning && blocklypy.isConnected"
        }
      ],
      "debug/toolBar": [
        {
          "command": "blocklypy.startUserProgram",
          "group": "navigation",
          "when": "(editorLangId == python || activeCustomEditorId == blocklypy-blocklypyViewer) && !blocklypy.isRunning && blocklypy.isConnected"
        },
        {
          "command": "blocklypy.stopUserProgram",
          "group": "navigation",
          "when": "(editorLangId == python || activeCustomEditorId == blocklypy-blocklypyViewer) && blocklypy.isRunning && blocklypy.isConnected"
        }
      ],
      "editor/title": [
        {
          "command": "blocklypy.blocklypyViewer.displayPreview",
          "when": "activeCustomEditorId == blocklypy-blocklypyViewer && blocklypy.customViewType !== 'preview' && blocklypy.contentAvailability.hasPreview",
          "group": "navigation@2"
        },
        {
          "command": "blocklypy.blocklypyViewer.displayPseudo",
          "when": "activeCustomEditorId == blocklypy-blocklypyViewer && blocklypy.customViewType !== 'pseudo' && blocklypy.contentAvailability.hasPseudo",
          "group": "navigation@3"
        },
        {
          "command": "blocklypy.blocklypyViewer.displayPycode",
          "when": "activeCustomEditorId == blocklypy-blocklypyViewer && blocklypy.customViewType !== 'pycode' && blocklypy.contentAvailability.hasPycode",
          "group": "navigation@5"
        },
        {
          "command": "blocklypy.blocklypyViewer.displayGraph",
          "when": "activeCustomEditorId == blocklypy-blocklypyViewer && blocklypy.customViewType !== 'graph' && blocklypy.contentAvailability.hasGraph",
          "group": "navigation@6"
        },
        {
          "command": "blocklypy.blocklypyViewer.displayNextView",
          "when": "activeCustomEditorId == blocklypy-blocklypyViewer"
        },
        {
          "command": "blocklypy.blocklypyViewer.displayPreviousView",
          "when": "activeCustomEditorId == blocklypy-blocklypyViewer"
        },
        {
          "command": "blocklypy.showPythonPreview",
          "when": "editorLangId == python",
          "group": "navigation"
        },
        {
          "command": "blocklypy.pythonPreview.showSource",
          "when": "activeCustomEditorId == blocklypy-pythonPreview",
          "group": "navigation@1"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "blocklypy-blocklypyViewer",
        "displayName": "BlocklyPy Viewer",
        "icon": {
          "light": "asset/icons/file-lego-light.svg",
          "dark": "asset/icons/file-lego-dark.svg"
        },
        "selector": [
          {
            "filenamePattern": "*.{llsp3,llsp,lms,lmsp,ev3,ev3m,rbf,proj}"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "blocklypy-pythonPreview",
        "displayName": "Python Dependency Graph Preview",
        "priority": "option",
        "icon": {
          "light": "asset/icons/display-graph-light.svg",
          "dark": "asset/icons/display-graph-dark.svg"
        },
        "selector": [
          {
            "filenamePattern": "*.py"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "lego",
        "extensions": [
          ".llsp3",
          ".llsp",
          ".lmsp",
          ".lms",
          ".ev3",
          ".ev3m",
          ".rbf",
          ".proj"
        ],
        "aliases": [
          "LEGO",
          "lego"
        ],
        "icon": {
          "light": "asset/icons/file-lego-light.svg",
          "dark": "asset/icons/file-lego-dark.svg"
        }
      }
    ],
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/spikehub1.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "compile-dev": "rm -rf dist && NODE_ENV=development webpack",
    "watch": "NODE_ENV=development webpack --watch --config-node-env development --devtool source-map",
    "watch-dev": "NODE_ENV=development webpack --watch --config-node-env development --devtool source-map",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "postinstall": "patch-package",
    "package-extension": "npx vsce package",
    "publish-extension": "npx vsce publish",
    "publish-extension-with-patch": "npx vsce publish patch"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/resolve": "^1.20.6",
    "@types/semver": "^7.7.1",
    "@types/vscode": "^1.101.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "debug": "^4.4.3",
    "electron": "^38.1.2",
    "eslint": "^9.25.1",
    "jest": "^30.1.3",
    "monaco-editor": "^0.52.2",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "patch-package": "^8.0.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@hpcc-js/wasm-graphviz": "^1.12.0",
    "@pybricks/mpy-cross-v6": "^2.0.0",
    "@pybricks/python-program-analysis": "^2.0.0",
    "@stoprocent/noble": "^2.3.6",
    "blocklypy": "^0.1.9",
    "crc-32": "^1.2.2",
    "lodash": "^4.17.21",
    "resolve": "^1.22.10",
    "semver": "^7.7.2",
    "serialport": "^12.0.0",
    "svg-pan-zoom": "^3.6.2",
    "uplot": "^1.6.32",
    "usb": "^2.16.0"
  }
}
