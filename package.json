{
  "name": "blocklypy-vscode",
  "displayName": "BlocklyPy Commander for LEGO Robots",
  "description": "Interact with LEGOÂ® Hubs running Pybricks firmware: compile and upload Python scripts, convert LEGO robotics files.",
  "version": "0.7.9",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "pybricks",
    "blocklypy",
    "blpy",
    "lego",
    "ev3",
    "spike",
    "spike prime",
    "mindstorms",
    "robotics",
    "micropython",
    "python",
    "visual programming",
    "first lego league",
    "fll",
    "education",
    "world robot olympiad",
    "wro"
  ],
  "publisher": "afarago",
  "sponsor": {
    "url": "https://www.github.com/sponsors/afarago",
    "label": "Sponsor"
  },
  "homepage": "https://attilafarago.hu",
  "repository": {
    "type": "git",
    "url": "https://github.com/afarago/blocklypy-vscode.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afarago/blocklypy-vscode/issues"
  },
  "icon": "icon.png",
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:lego",
    "onWebviewPanel:blocklypy-vscode-blocklypyViewer",
    "onWebviewPanel:blocklypy-vscode-pythonPreview",
    "onWebviewPanel:blocklypy-vscode-datalogview",
    "onDebugResolve:pybricks-tunnel",
    "onDebugDynamicConfigurations:pybricks-tunnel"
  ],
  "main": "./dist/extension.js",
  "packageManager": "yarn@1.22.19",
  "files": [
    "dist",
    "asset/",
    "screenshot/"
  ],
  "extensionPack": [
    "ms-python.python",
    "ReprEng.csv"
  ],
  "contributes": {
    "commands": [
      {
        "command": "blocklypy-vscode.helloWorld-debug-in-editor",
        "title": "helloWorld-2"
      },
      {
        "command": "blocklypy-vscode.helloWorld-trapline",
        "title": "helloWorld-3-trapline"
      },
      {
        "command": "blocklypy-vscode.helloWorld",
        "title": "helloWorld"
      },
      {
        "command": "blocklypy-vscode.connectDevice",
        "title": "Connect Device",
        "category": "BlocklyPy Commander",
        "icon": {
          "light": "asset/icons/hub-small-light.svg",
          "dark": "asset/icons/hub-small-dark.svg"
        },
        "enablement": "true || !blocklypy-vscode.isConnected"
      },
      {
        "command": "blocklypy-vscode.disconnectDevice",
        "title": "Disconnect Device",
        "category": "BlocklyPy Commander",
        "icon": "$(debug-disconnect)",
        "enablement": "blocklypy-vscode.isConnected"
      },
      {
        "command": "blocklypy-vscode.compile",
        "title": "Compile",
        "category": "BlocklyPy Commander",
        "icon": "$(compile)",
        "enablement": "!inDebugMode && (blocklypy-vscode.activeEditorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer)"
      },
      {
        "command": "blocklypy-vscode.compileAndRun",
        "title": "Compile and Run",
        "category": "BlocklyPy Commander",
        "icon": "$(debug-start)",
        "enablement": "!inDebugMode && (blocklypy-vscode.activeEditorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer) && blocklypy-vscode.isConnected"
      },
      {
        "command": "blocklypy-vscode.compileAndRunWithDebug",
        "title": "Compile and Debug",
        "category": "BlocklyPy Commander",
        "icon": "$(debug-alt)",
        "enablement": "!inDebugMode && (blocklypy-vscode.activeEditorLangId == python) && blocklypy-vscode.isConnected && blocklypy-vscode.ConnectedDeviceType =~ /pybricks/"
      },
      {
        "command": "blocklypy-vscode.startUserProgram",
        "title": "Start User Program",
        "category": "BlocklyPy Commander",
        "icon": "$(run)",
        "enablement": "!inDebugMode && (blocklypy-vscode.activeEditorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer) && blocklypy-vscode.isConnected && !blocklypy-vscode.isRunning"
      },
      {
        "command": "blocklypy-vscode.stopUserProgram",
        "title": "Stop User Program",
        "category": "BlocklyPy Commander",
        "icon": "$(debug-stop)",
        "enablement": "blocklypy-vscode.isConnected && blocklypy-vscode.isRunning"
      },
      {
        "command": "blocklypy-vscode.blocklypyViewer.displayNextView",
        "title": "Next View",
        "category": "BlocklyPy Commander",
        "icon": "$(preview)",
        "enablement": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer"
      },
      {
        "command": "blocklypy-vscode.blocklypyViewer.displayPreviousView",
        "title": "Previous View",
        "category": "BlocklyPy Commander",
        "icon": "$(preview)",
        "enablement": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer"
      },
      {
        "command": "blocklypy-vscode.blocklypyViewer.displayPycode",
        "title": "Show Python Code",
        "category": "BlocklyPy Commander",
        "icon": {
          "light": "asset/icons/display-py-light.svg",
          "dark": "asset/icons/display-py-dark.svg"
        },
        "enablement": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer && blocklypy-vscode.customViewType !== 'pycode' && blocklypy-vscode.contentAvailability.hasPycode"
      },
      {
        "command": "blocklypy-vscode.blocklypyViewer.displayPseudo",
        "title": "Show Pseudo Code",
        "category": "BlocklyPy Commander",
        "icon": {
          "light": "asset/icons/display-pseudo-light.svg",
          "dark": "asset/icons/display-pseudo-dark.svg"
        },
        "enablement": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer && blocklypy-vscode.customViewType !== 'pseudo' && blocklypy-vscode.contentAvailability.hasPseudo"
      },
      {
        "command": "blocklypy-vscode.blocklypyViewer.displayPreview",
        "title": "Show Preview",
        "category": "BlocklyPy Commander",
        "icon": {
          "light": "asset/icons/display-preview-light.svg",
          "dark": "asset/icons/display-preview-dark.svg"
        },
        "enablement": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer && blocklypy-vscode.customViewType !== 'preview' && blocklypy-vscode.contentAvailability.hasPreview"
      },
      {
        "command": "blocklypy-vscode.blocklypyViewer.displayGraph",
        "title": "Show Dependency Graph",
        "category": "BlocklyPy Commander",
        "icon": {
          "light": "asset/icons/display-graph-light.svg",
          "dark": "asset/icons/display-graph-dark.svg"
        },
        "enablement": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer && blocklypy-vscode.customViewType !== 'graph' && blocklypy-vscode.contentAvailability.hasGraph"
      },
      {
        "command": "blocklypy-vscode.showPythonPreview",
        "title": "Show Python Dependency Preview",
        "category": "BlocklyPy Commander",
        "icon": {
          "light": "asset/icons/display-graph-light.svg",
          "dark": "asset/icons/display-graph-dark.svg"
        },
        "enablement": "blocklypy-vscode.activeEditorLangId == python"
      },
      {
        "command": "blocklypy-vscode.pythonPreview.showSource",
        "title": "Show Python Source Code",
        "category": "BlocklyPy Commander",
        "icon": "$(go-to-file)",
        "enablement": "activeCustomEditorId == blocklypy-vscode-pythonPreview"
      },
      {
        "command": "blocklypy-vscode.moveSlot",
        "title": "Move Program between Slots in HubOS",
        "category": "BlocklyPy Commander",
        "icon": "$(arrow-right)",
        "enablement": "blocklypy-vscode.isConnected && blocklypy-vscode.ConnectedDeviceType =~ /hubos/"
      },
      {
        "command": "blocklypy-vscode.clearSlot",
        "title": "Clear a Slot in HubOS",
        "category": "BlocklyPy Commander",
        "icon": "$(trash)",
        "enablement": "blocklypy-vscode.isConnected && blocklypy-vscode.ConnectedDeviceType =~ /hubos/"
      },
      {
        "command": "blocklypy-vscode.clearAllSlots",
        "title": "Clear All Slots in HubOS",
        "category": "BlocklyPy Commander",
        "icon": "$(trash)",
        "enablement": "blocklypy-vscode.isConnected && blocklypy-vscode.ConnectedDeviceType =~ /hubos/"
      },
      {
        "command": "blocklypy-vscode.startScanning",
        "title": "Start Scanning",
        "category": "BlocklyPy Commander",
        "icon": "$(eye)",
        "enablement": "!blocklypy-vscode.isScanning"
      },
      {
        "command": "blocklypy-vscode.stopScanning",
        "title": "Stop Scanning",
        "category": "BlocklyPy Commander",
        "icon": "$(eye-closed)",
        "enablement": "blocklypy-vscode.isScanning"
      },
      {
        "command": "blocklypy-vscode.datalogOpenCSV",
        "title": "Open Data Log CSV",
        "category": "BlocklyPy Commander",
        "icon": "$(file-symlink-file)",
        "enablement": "blocklypy-vscode.isPlotDataAvailable"
      },
      {
        "command": "blocklypy-vscode.datalogClear",
        "title": "Clear Data Log",
        "category": "BlocklyPy Commander",
        "icon": "$(trash)",
        "enablement": "blocklypy-vscode.isPlotDataAvailable"
      },
      {
        "command": "blocklypy-vscode.promptDeviceNotificationPlotFilter",
        "title": "Set Device Notification Plot Filter",
        "category": "BlocklyPy Commander",
        "icon": "$(filter)",
        "enablement": "blocklypy-vscode.isConnected"
      },
      {
        "command": "blocklypy-vscode.startREPL",
        "title": "Start Python REPL",
        "category": "BlocklyPy Commander",
        "icon": "$(terminal)"
      },
      {
        "command": "blocklypy-vscode.startHubMonitor",
        "title": "Start Hub Monitor",
        "category": "BlocklyPy Commander",
        "icon": "$(graph-line)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "blocklypy-vscode-commands",
          "title": "BlocklyPy Commander",
          "icon": "asset/icons/icon.svg"
        }
      ],
      "panel": [
        {
          "id": "blocklypy-vscode-datalogview",
          "title": "Datalog",
          "icon": "$(graph-line)"
        }
      ],
      "secondarySidebar": []
    },
    "views": {
      "blocklypy-vscode-datalogview": [
        {
          "id": "blocklypy-vscode-datalogview",
          "name": "Chart",
          "icon": "$(graph-line)",
          "type": "webview"
        }
      ],
      "blocklypy-vscode-commands": [
        {
          "id": "blocklypy-vscode-commands",
          "name": "",
          "icon": "asset/icons/icon.svg"
        }
      ]
    },
    "keybindings": [
      {
        "command": "blocklypy-vscode.compileAndRun",
        "key": "f5",
        "when": "(editorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer)"
      },
      {
        "command": "blocklypy-vscode.compileAndRunWithDebug",
        "key": "ctrl+f5",
        "when": "(editorLangId == python)"
      },
      {
        "command": "blocklypy-vscode.compile",
        "key": "shift+ctrl+f5",
        "when": "(editorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer)"
      },
      {
        "command": "blocklypy-vscode.stopUserProgram",
        "key": "f6"
      },
      {
        "command": "blocklypy-vscode.stopUserProgram",
        "key": "shift+f5"
      },
      {
        "command": "blocklypy-vscode.blocklypyViewer.displayNextView",
        "key": "f7",
        "when": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer"
      },
      {
        "command": "blocklypy-vscode.blocklypyViewer.displayPreviousView",
        "key": "shift+f7",
        "when": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer"
      },
      {
        "command": "blocklypy-vscode.showPythonPreview",
        "key": "f7",
        "when": "editorLangId == python"
      },
      {
        "command": "blocklypy-vscode.pythonPreview.showSource",
        "key": "f7",
        "when": "activeCustomEditorId == blocklypy-vscode-pythonPreview"
      }
    ],
    "configuration": {
      "title": "BlocklyPy",
      "properties": {
        "blocklypy-vscode.lastconnected-device-name": {
          "type": "string",
          "default": "",
          "description": "The name of the last connected device."
        },
        "blocklypy-vscode.autoconnect-last-device": {
          "type": "boolean",
          "default": true,
          "description": "Automatically connect to the last connected device."
        },
        "blocklypy-vscode.autoclear-terminal": {
          "type": "boolean",
          "default": false,
          "description": "Automatically clear the terminal before compiling and running a program."
        },
        "blocklypy-vscode.connection-timeout": {
          "type": "number",
          "default": 15000,
          "minimum": 5000,
          "description": "The timeout in milliseconds for connecting to a device."
        },
        "blocklypy-vscode.device-visibility-timeout": {
          "type": "number",
          "default": 10000,
          "minimum": 1000,
          "description": "The time in milliseconds after which a device is considered not visible if not seen during scanning."
        },
        "blocklypy-vscode.device-notification-plot-filter": {
          "type": "string",
          "description": "A comma separated list of device states to filter HubOS device notifications for plotting. Example: ImuValues.yaw."
        },
        "blocklypy-vscode.feature-flags": {
          "type": "object",
          "description": "Feature flags for experimental or optional features.",
          "default": {
            "autostart-on-magicheader": true,
            "plot-data-from-stdout": true
          },
          "properties": {
            "autostart-on-magicheader": {
              "type": "boolean",
              "description": "Automatically start the user program if a magic header is detected in the output. Example: `# LEGO slot:0 autostart`"
            },
            "log-device-notification": {
              "type": "boolean",
              "description": "Log a device state notifications using the HubOS protocol."
            },
            "plot-device-notification": {
              "type": "boolean",
              "description": "Plot a device state notification using the HubOS protocol."
            },
            "log-tunnel-notification": {
              "type": "boolean",
              "description": "Log a tunnel message notification using the HubOS protocol."
            },
            "plot-data-from-stdout": {
              "type": "boolean",
              "description": "Parse the stdout of the user program for data to plot. Example output: `print('plot: x=1,y=2')` or `# plot(x)`"
            },
            "autoconnect-first-usb-device": {
              "type": "boolean",
              "description": "Automatically connect to the first USB device found."
            },
            "pybricks-application-interface-for-pybricks-protocol": {
              "type": "boolean",
              "description": "Use the Application Interface for the Pybricks Protocol though Pybricks AppData (debug, plot, ... etc.)"
            }
          }
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "blocklypy-vscode.startScanning",
          "group": "navigation@2",
          "when": "view == blocklypy-vscode-commands"
        },
        {
          "command": "blocklypy-vscode.stopScanning",
          "group": "navigation@2",
          "when": "view == blocklypy-vscode-commands"
        },
        {
          "command": "blocklypy-vscode.compileAndRun",
          "group": "navigation@1",
          "when": "view == blocklypy-vscode-commands"
        },
        {
          "command": "blocklypy-vscode.stopUserProgram",
          "group": "navigation@1",
          "when": "view == blocklypy-vscode-commands"
        },
        {
          "command": "blocklypy-vscode.datalogOpenCSV",
          "group": "navigation@2",
          "when": "view == blocklypy-vscode-datalogview"
        },
        {
          "command": "blocklypy-vscode.promptDeviceNotificationPlotFilter",
          "group": "navigation@1",
          "when": "view == blocklypy-vscode-datalogview && blocklypy-vscode.isConnected"
        },
        {
          "command": "blocklypy-vscode.datalogClear",
          "group": "navigation@3",
          "when": "view == blocklypy-vscode-datalogview && blocklypy-vscode.isPlotDataAvailable"
        }
      ],
      "editor/title/run": [
        {
          "command": "blocklypy-vscode.compileAndRun",
          "group": "navigation",
          "when": "(editorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer)"
        },
        {
          "command": "blocklypy-vscode.stopUserProgram",
          "group": "navigation",
          "when": "(editorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer)"
        }
      ],
      "debug/toolBar": [
        {
          "command": "blocklypy-vscode.startUserProgram",
          "group": "navigation",
          "when": "(editorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer)"
        },
        {
          "command": "blocklypy-vscode.stopUserProgram",
          "group": "navigation",
          "when": "(editorLangId == python || activeCustomEditorId == blocklypy-vscode-blocklypyViewer)"
        }
      ],
      "editor/title": [
        {
          "command": "blocklypy-vscode.blocklypyViewer.displayPreview",
          "when": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer",
          "group": "navigation@2"
        },
        {
          "command": "blocklypy-vscode.blocklypyViewer.displayPseudo",
          "when": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer",
          "group": "navigation@3"
        },
        {
          "command": "blocklypy-vscode.blocklypyViewer.displayPycode",
          "when": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer",
          "group": "navigation@5"
        },
        {
          "command": "blocklypy-vscode.blocklypyViewer.displayGraph",
          "when": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer",
          "group": "navigation@6"
        },
        {
          "command": "blocklypy-vscode.blocklypyViewer.displayNextView",
          "when": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer"
        },
        {
          "command": "blocklypy-vscode.blocklypyViewer.displayPreviousView",
          "when": "activeCustomEditorId == blocklypy-vscode-blocklypyViewer"
        },
        {
          "command": "blocklypy-vscode.showPythonPreview",
          "when": "editorLangId == python",
          "group": "navigation"
        },
        {
          "command": "blocklypy-vscode.pythonPreview.showSource",
          "when": "activeCustomEditorId == blocklypy-vscode-pythonPreview",
          "group": "navigation@1"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "blocklypy-vscode-blocklypyViewer",
        "displayName": "BlocklyPy Viewer",
        "icon": {
          "light": "asset/icons/file-lego-light.svg",
          "dark": "asset/icons/file-lego-dark.svg"
        },
        "selector": [
          {
            "filenamePattern": "*.{llsp3,llsp,lms,lmsp,ev3,ev3m,rbf,proj}"
          }
        ],
        "priority": "default"
      },
      {
        "viewType": "blocklypy-vscode-pythonPreview",
        "displayName": "Python Dependency Graph Preview",
        "priority": "option",
        "icon": {
          "light": "asset/icons/display-graph-light.svg",
          "dark": "asset/icons/display-graph-dark.svg"
        },
        "selector": [
          {
            "filenamePattern": "*.py"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "lego",
        "extensions": [
          ".llsp3",
          ".llsp",
          ".lmsp",
          ".lms",
          ".ev3",
          ".ev3m",
          ".rbf",
          ".proj"
        ],
        "aliases": [
          "LEGO",
          "lego"
        ],
        "icon": {
          "light": "asset/icons/file-lego-light.svg",
          "dark": "asset/icons/file-lego-dark.svg"
        }
      }
    ],
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/spikehub1.json"
      }
    ],
    "breakpoints": [
      {
        "language": "python"
      }
    ],
    "debuggers": [
      {
        "type": "pybricks-tunnel",
        "languages": [
          "python",
          "lego"
        ],
        "label": "Pybricks Tunnel Debugger",
        "runtime": "node",
        "configurationAttributes": {
          "launch": {
            "required": [
              "program"
            ],
            "properties": {
              "program": {
                "type": "string",
                "description": "Absolute path to a text file.",
                "default": "${workspaceFolder}/${command:AskForProgramName}"
              },
              "stopOnEntry": {
                "type": "boolean",
                "description": "Automatically stop after launch.",
                "default": true
              },
              "slot": {
                "type": "number",
                "description": "The slot number to use when uploading the program (HubOS only, 0-19).",
                "default": 0,
                "minimum": 0,
                "maximum": 19
              },
              "compiled": {
                "type": "boolean",
                "description": "Whether the program is pre-compiled to .mpy format (true) or raw Python source (false) (HubOS only).",
                "default": true
              }
            }
          },
          "initialConfigurations": [
            {
              "type": "pybricks-tunnel",
              "request": "launch",
              "name": "Ask for file name",
              "program": "${workspaceFolder}/${command:AskForProgramName}",
              "stopOnEntry": true
            }
          ],
          "configurationSnippets": [
            {
              "label": "Pybricks Tunnel Debug: Launch",
              "description": "A new configuration for 'debugging' a user selected pythom file.",
              "body": {
                "type": "pybricks-tunnel",
                "request": "launch",
                "name": "Ask for file name",
                "program": "^\"\\${workspaceFolder}/\\${command:AskForProgramName}\"",
                "stopOnEntry": true
              }
            }
          ],
          "variables": {
            "AskForProgramName": "blocklypy-vscode.getProgramName"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "compile-dev": "rm -rf dist && NODE_ENV=development webpack",
    "watch": "NODE_ENV=development webpack --watch --config-node-env development --devtool source-map",
    "watch-dev": "NODE_ENV=development webpack --watch --config-node-env development --devtool source-map",
    "package": "rm -rf dist && webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "postinstall": "patch-package",
    "package-extension": "npx vsce package",
    "publish-extension": "npx vsce publish",
    "publish-extension-with-patch": "npx vsce publish patch"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/resolve": "^1.20.6",
    "@types/semver": "^7.7.1",
    "@types/vscode": "^1.88.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
